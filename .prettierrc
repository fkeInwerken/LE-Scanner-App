{
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"useTabs": false,
	"trailingComma": "all",
	"printWidth": 150,
	"arrowParens": "always",
	"bracketSpacing": true,
	"htmlWhitespaceSensitivity": "ignore",
	"jsxBracketSameLine": false,
	"endOfLine": "lf",
	"proseWrap": "never",
	"embeddedLanguageFormatting": "auto"
}
